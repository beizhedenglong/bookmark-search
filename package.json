{
  "name": "bookmark-search",
  "version": "1.0.0",
  "description": "A Chrome extension for searching bookmark quickly.",
  "main": "src/popup.js",
  "scripts": {
    "watch": "parcel watch --no-hmr ./src/popup.html -d extension/dist --no-source-maps  --public-url ./",
    "build": "yarn clean & parcel build ./src/popup.html -d extension/dist --no-source-maps  --public-url ./",
    "clean": "rm -rf ./extension/dist ./dist ./.cache",
    "lint": "eslint ./src"
  },
  "author": {
    "name": "Victor",
    "email": "wang_shaojie@icloud.com",
    "url": "https://github.com/beizhedenglong"
  },
  "bugs": {
    "url": "https://github.com/beizhedenglong/bookmark-search/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beizhedenglong/bookmark-search"
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^3.10.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-eslint": "^8.2.6",
    "babel-plugin-import": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "parcel-bundler": "^1.10.3"
  }
}
